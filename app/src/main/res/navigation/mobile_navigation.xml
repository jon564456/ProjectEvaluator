<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/nav_btn_evaluador">
    <fragment
        android:id="@+id/nav_btn_evaluador"
        android:name="com.itsoeh.jbrigido.projectevaluator.ui.evaluador.EvaluadorFragment"
        android:label="Evaluadores"
        tools:layout="@layout/fragment_frag_evaluador" />
    <fragment
        android:id="@+id/nav_btn_proyecto"
        android:name="com.itsoeh.jbrigido.projectevaluator.ui.proyecto.ProyectoFragment"
        android:label="Proyectos"
        tools:layout="@layout/fragment_frag_proyecto" />
    <fragment
        android:id="@+id/frag_view_info_eva"
        android:name="com.itsoeh.jbrigido.projectevaluator.ui.infoevaluador.InfoEvaluadorFragment"
        android:label="Información del evaluador"
        tools:layout="@layout/fragment_frag_view_info_eva" />
    <fragment
        android:id="@+id/frag_view_info_proyecto"
        android:name="com.itsoeh.jbrigido.projectevaluator.ui.infoproyecto.InfoProyectoEvaluador"
        android:label="Información del proyecto"
        tools:layout="@layout/fragment_frag_view_info_proyecto" />
    <fragment
        android:id="@+id/infoFragment"
        android:name="com.itsoeh.jbrigido.projectevaluator.ui.info.InfoFragment"
        android:label="Información de desarrrollo"
        tools:layout="@layout/fragment_frag_info" />
    <fragment
        android:id="@+id/nav_btn_atributos"
        android:name="com.itsoeh.jbrigido.projectevaluator.ui.atributos.Fragment_Atributos"
        android:label="Atributos"
        tools:layout="@layout/fragment_atributos" />
    <fragment
        android:id="@+id/nav_btn_opciones"
        android:name="com.itsoeh.jbrigido.projectevaluator.ui.opciones.Fragment_opciones"
        android:label="Opciones"
        tools:layout="@layout/fragment_opciones" >
        <action
            android:id="@+id/action_nav_btn_opciones_to_infoFragment"
            app:destination="@id/infoFragment" />
        <action
            android:id="@+id/action_nav_btn_opciones_to_resultadosFragment"
            app:destination="@id/resultadosFragment" />
    </fragment>
    <fragment
        android:id="@+id/nav_btn_categoorias"
        android:name="com.itsoeh.jbrigido.projectevaluator.ui.categoria.Fragment_Categoria"
        android:label="Categorias"
        tools:layout="@layout/fragment_categoria" />
    <fragment
        android:id="@+id/resultadosFragment"
        android:name="com.itsoeh.jbrigido.projectevaluator.ui.resultados.ResultadosFragment"
        android:label="Resultados"
        tools:layout="@layout/fragment_resultados" />
</navigation>